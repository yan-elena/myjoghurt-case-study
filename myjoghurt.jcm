mas myjoghurt {

    agent plant {
        ag-class: npl.NormativeAg("src/npl/plant_de_jure.npl")
    }

    agent unit {
        ag-class: npl.NormativeAg("src/npl/unit_de_jure.npl")
        join: plant
        focus: valve
    }

    agent container {
       join: plant
       focus: valve
    }

    agent bottle

    workspace plant {
        artifact valve: ValveArtifact(200)
    }
}
